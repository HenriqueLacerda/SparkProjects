CREATE TABLE "enterprises" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "nome" varchar(255), "endereco" varchar(255), "numero" varchar(255), "bairro" varchar(255), "cidade" varchar(255), "estado" varchar(255), "cep" varchar(255), "logo" blob, "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL);
CREATE TABLE "priorities" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "nome" varchar(255), "ordem" integer, "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL);
